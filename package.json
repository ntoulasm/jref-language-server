{
  "name": "jref-language-server",
  "displayName": "jref-language-server",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./client/out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jref-language-server.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "workspaces": [
    "client"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b ./",
    "watch": "tsc -watch -b ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "npm run lint -w client",
    "test": "npm test -w client"
  },
  "devDependencies": {
    "@types/node": "22.x",
    "typescript-eslint": "^8.54.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3"
  }
}
